   "source": [
    "#미분 방정식 계산\n",
    "from sympy import *\n",
    "import math\n",
    "init_printing(use_latex='mathjax')\n",
    "t = symbols('t')\n",
    "a = symbols('a')\n",
    "x = Function('x')\n",
    "x,x(t).diff(t)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$$\\frac{d}{d t} x{\\left (t \\right )} = - a x{\\left (t \\right )}$$"
      ],
      "text/plain": [
       "d                 \n",
       "──(x(t)) = -a⋅x(t)\n",
       "dt                "
      ]
     },
     "execution_count": 2,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "deq = Eq(x(t).diff(t),-a*x(t))\n",
    "deq"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$$x{\\left (t \\right )} = C_{1} e^{- a t}$$"
      ],
      "text/plain": [
       "           -a⋅t\n",
       "x(t) = C₁⋅ℯ    "
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "dsolve(deq,x(t))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/latex": [
       "$$x{\\left (t \\right )} = e^{- a t}$$"
      ],
      "text/plain": [
       "        -a⋅t\n",
       "x(t) = ℯ    "
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "psol = dsolve( deq, ics= {x(0):1} )\n",
    "psol"
   ]
